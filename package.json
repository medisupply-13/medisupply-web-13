{
  "name": "medisupply",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json,md}\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:headless": "cypress run --headless",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:edge": "cypress run --browser edge"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.4",
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/google-maps": "^20.2.7",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@swimlane/ngx-charts": "^23.0.1",
    "d3": "^7.9.0",
    "d3-array": "^3.2.4",
    "d3-shape": "^3.2.0",
    "recharts": "^3.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/localize": "^20.3.2",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.1",
    "angular-eslint": "20.3.0",
    "cypress": "^15.6.0",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "^6.3.6"
  }
}
